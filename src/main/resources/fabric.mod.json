{
	"schemaVersion": 1,
	"id": "fabric-scheduler",
	"version": "${version}",
	"name": "Fabric Scheduler",
	"description": "Fabric Scheduler is a direct port of Bukkit's Scheduler, on its own it doesn't make any changes, its purpose is to be used in the development of mods / plugins.",
	"authors": [
		"Bati"
	],
	"contact": {
		"discord": "Bati#4439",
		"github": "https://github.com/Bati2ra"
	},
	"license": "CC0-1.0",
	"icon": "assets/fabric-scheduler/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "com.bati.fabricscheduler.Scheduler"
		]
	},
	"mixins": [
		"fabric-scheduler.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.19.2",
		"java": ">=17",
		"fabric-api": "*"
